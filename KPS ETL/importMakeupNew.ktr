  <step>
    <name>Database join GET COST_PRICE_ACCUM</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>CHEMIDB</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT SUM(M1.MUQTY*COALESCE(CC.SG,1)*COALESCE(CCR.UNITPRICE,0)) as COST_PRICE_ACCUM
  FROM CHEMIMAKEUP AS M1
	LEFT JOIN CHEMICAL CC ON CC.CHEMICODE = M1.CHEMICODE
	LEFT JOIN CHEMICALCONTROLPRICE CCR ON 
						 CCR.CHEMICODE = M1.CHEMICODE and
					(
							(1 = (SELECT sum(COALESCE(ISUSEPRICE,0)) FROM CHEMICALCONTROLPRICE WHERE  CHEMICODE =  ?) 
									 AND CCR.ISUSEPRICE = 1
							) 
							OR 
							(0 = (SELECT sum(COALESCE(ISUSEPRICE,0)) FROM CHEMICALCONTROLPRICE WHERE  CHEMICODE =  ?) 
									 AND (
										convert(date,FROMDATE ) &lt;= ?
										AND convert(date,EXPIRESDATE ) >= ?)
							)
					)
  where M1.linecode = ?
  and M1.pondcode = ?
  and M1.chemicode = ?
	and convert(date,M1.makeupdate) BETWEEN ? and ?;</sql>
