  <step>
    <name>Database join GET COST_PRICE</name>
    <type>DBJoin</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>CHEMIDB</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT
COALESCE(SG,2) as SG,COALESCE(CCR.UNITPRICE,0) as UNITPRICE, (? * COALESCE(SG,1) * (COALESCE(CCR.UNITPRICE,0))) as COST_PRICE
FROM CHEMICAL M1
LEFT JOIN CHEMICALCONTROLPRICE CCR ON 
						 CCR.CHEMICODE = M1.CHEMICODE and
					(
							(1 = (SELECT sum(COALESCE(ISUSEPRICE,0)) FROM CHEMICALCONTROLPRICE WHERE  CHEMICODE =  ?) 
									 AND ISUSEPRICE = 1
							) 
							OR 
							(0 = (SELECT sum(COALESCE(ISUSEPRICE,0)) FROM CHEMICALCONTROLPRICE WHERE  CHEMICODE =  ?) 
									 AND (
										convert(date,FROMDATE ) &lt;= ?
										AND convert(date,EXPIRESDATE ) >= ?)
							)
					)
WHERE M1.CHEMICODE = ?
</sql>
    <outer_join>Y</outer_join>
    <replace_vars>N</replace_vars>
