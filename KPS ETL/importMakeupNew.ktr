  <step>
    <name>SELECT CHEMIMAKEUP</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>CHEMIDB</connection>
    <sql>SELECT Q.LINECODE, 
 Q.LINENAME, 
 Q.PONDCODE, 
 Q.PONDNAME, 
 Q.CHEMICODE, 
 Q.CHEMINAME, 
 Q.MAKEUPDATE,
 Q.MAKEUPSTARTDATE,
 MIN(Q.MAKEUPTYPEID) MAKEUPTYPEID, 
 SUM(Q.MUQTY) MUQTY
FROM(
 SELECT CP.LINECODE
 , CP.LINENAME
 , CP.PONDCODE
 , CP.PONDNAME
 , CP.CHEMICODE
 , CP.CHEMINAME
 , CP.MAKEUPTYPEID
 , CP.MUQTY
 , CONVERT(date,CP.MAKEUPDATE) AS MAKEUPDATE
 , (
  SELECT MAKEUPSTARTDATE
  FROM MAKEUPDATE_START_END
  WHERE LINECODE = CP.LINECODE
  AND PONDCODE = CP.PONDCODE
  AND CP.MAKEUPDATE BETWEEN MAKEUPSTARTDATE AND ISNULL(MAKEUPENDDATE, GETDATE())
 ) MAKEUPSTARTDATE
 FROM CHEMIMAKEUP AS CP
 WHERE CAST(CP.MAKEUPDATE AS DATE) BETWEEN '${param_start_date}' AND '${param_end_date}'
 AND EXISTS (
  SELECT 1
  FROM MAKEUPDATE_START_END
  WHERE LINECODE = CP.LINECODE
  AND PONDCODE = CP.PONDCODE
  AND CP.MAKEUPDATE BETWEEN MAKEUPSTARTDATE AND ISNULL(MAKEUPENDDATE, GETDATE())
 )
)Q
GROUP BY Q.LINECODE, 
 Q.LINENAME, 
 Q.PONDCODE, 
 Q.PONDNAME, 
 Q.CHEMICODE, 
 Q.CHEMINAME, 
 Q.MAKEUPDATE,
 Q.MAKEUPSTARTDATE
ORDER BY Q.LINECODE, 
 Q.LINENAME, 
 Q.PONDCODE, 
 Q.PONDNAME, 
 Q.CHEMICODE, 
 Q.CHEMINAME, 
 Q.MAKEUPDATE,
 Q.MAKEUPSTARTDATE</sql>
